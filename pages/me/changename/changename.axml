<import src="/pages/shelf/footer/footer.axml"/>
<view class="page">
  <view class="page-content">
    <notice show="{{ifShow}}" type="error">
    {{notice_msg}}
    </notice>
    <input-item data-field="new_username" placeholder="请输入新的用户名" value="{{userInfo.username}}" onInput="onItemInput" onClear="onClear">用户名</input-item>
    <button onTap="changeName" type="primary" disabled="{{disabled}}" data-input="{{new_name}}">
    修改
    </button>
  </view>
  <template is="footer"/>
</view>